    if test "$with_libode" != no; then
  # If --with-lib value is same as cached value, then assume other
	 # cached values are also valid; otherwise, ignore all cached values.
	if test "$with_libode" != "$cs_cv_with_libode"; then
  cs_ignore_cache=yes
else
  cs_ignore_cache=no
