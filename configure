    set filler `echo "$cs_build_item" | awk 'BEGIN { FS="@" }
	{ for (i=1; i <= NF; ++i)
	    { if ($i == "") print "filler"; else print $i } }'`
    shift
    if test $# -eq 0; then
  cs_dummy=''
else
  if test "$1" = filler; then
  cs_dummy=''
else
  cs_dummy=$1
