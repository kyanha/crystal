    cs_check_lib_flags="$cs_check_lib_flags `echo @$cs_check_lib_cflag@$cs_check_lib_lflag@$cs_check_lib_libs | sed 'y% %#%'`"
		;;
	    *)  if test -n "$cs_lib_item"; then
  cs_check_lib_cflag="-I$cs_lib_item"
else
  cs_check_lib_cflag=''
fi

    if test -n "$cs_lib_item"; then
  cs_check_lib_lflag="-L$cs_lib_item"
