SubDir TOP libs csutil win32 ;

{
  local rejects ;
  if $(LIBBFD.AVAILABLE) != "yes"
  {
    rejects += bfd-module-win32.h ;
  }

  CsLibAddModule $(rejects) ;
}

if $(LIBBFD.AVAILABLE) = "yes"
{
  CSUTIL.GENERIC += callstack-bfd.cpp ;
}

CSUTIL.GENERIC +=
  appdir.cpp
  csprocessorcap.cpp
  findlib.cpp
  getopt.cpp
  pluginpaths.cpp
  resdir.cpp
  runloop.cpp ;
