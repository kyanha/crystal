SubDir TOP apps demo ;

if $(CEGUI.AVAILABLE) = "yes"
{
Description csdemo : "Demo" ;
Application csdemo : [ Wildcard *.cpp *.h ] ;
CFlags csdemo : $(COMPILER.C++FLAGS.EXCEPTIONS.ENABLE) ;
LinkWith csdemo : crystalspace ;
ExternalLibs csdemo : CEGUI ;
FileListEntryApplications csdemo : app-demo ;
}
