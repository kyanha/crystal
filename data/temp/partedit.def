window "Another"
{
  Frame: (100,100)-(600,250)
  Style: wfsNormal
  Title: "Particle Editor"

  connect
  {
    signalComponentCreated -> parteditSink::RegisterFrame
  }

  component "LabelX" is "Label"
  {
    Frame: (0, 10) - (40, 30)
    Align: lblAlignCenter
    Caption: "X:"
    connect 
    {
      signalComponentCreated -> parteditSink::RegisterLabel1
    }
  }
  component "LabelY" is "Label"
  {
    Frame: (0, 35) - (40, 55)
    Align: lblAlignCenter
    Caption: "Y:"
    connect 
    {
      signalComponentCreated -> parteditSink::RegisterLabel2
    }
  }
  component "LabelZ" is "Label"
  {
    Frame: (0, 60) - (40, 80)
    Align: lblAlignCenter
    Caption: "Z:"
    connect 
    {
      signalComponentCreated -> parteditSink::RegisterLabel3
    }
  }
  component "LabelX" is "Label"
  {
    Frame: (0, 85) - (40, 105)
    Align: lblAlignCenter
    Caption: "Other:"
    connect 
    {
      signalComponentCreated -> parteditSink::RegisterLabel4
    }
  }

  connect
  {
    signalComponentCreated -> awsStandardSink::Show
  }

  component "X" is "Scroll Bar"
  {
   Frame: (40, 10) - (140, 30)
   Orientation: sboHorizontal
   Min: 0
   Max: 1000
   Change: 1
   BigChange: 5

	connect
	{
	  signalScrollBarChanged -> parteditSink::SetScrollBarX
	}
  }
  component "Y" is "Scroll Bar"
  {
   Frame: (40, 35) - (140, 55)
   Orientation: sboHorizontal
   Min: 0
   Max: 1000
   Change: 1
   BigChange: 5

	connect
	{
	  signalScrollBarChanged -> parteditSink::SetScrollBarY
	}
  }
  component "Z" is "Scroll Bar"
  {
   Frame: (40, 60) - (140, 80)
   Orientation: sboHorizontal
   Min: 0
   Max: 1000
   Change: 2
   BigChange: 5

	connect
	{
	  signalScrollBarChanged -> parteditSink::SetScrollBarZ
	}
  }
  component "Other" is "Scroll Bar"
  {
   Frame: (40, 85) - (140, 105)
   Orientation: sboHorizontal
   Min: 0
   Max: 1000
   Change: 1
   BigChange: 5

	connect
	{
	  signalScrollBarChanged -> parteditSink::SetScrollBarOther
	}
  }
  component "Input1" is "Text Box"
  {
    Frame: (145, 10) - (190, 30)
    Style: tbfsNormal

    Disallow: "abcdefghijklmnopqrstuvwxz"
 
    connect 
    {
      signalComponentCreated -> parteditSink::RegisterInput1
      signalTextBoxLostFocus-> parteditSink::SetInput1
    }
  }
  component "Input2" is "Text Box"
  {
    Frame: (145, 35) - (190, 55)
    Style: tbfsNormal
    Disallow: "abcdefghijklmnopqrstuvwxz"

    connect 
    {
      signalComponentCreated -> parteditSink::RegisterInput2
      signalTextBoxLostFocus-> parteditSink::SetInput2
    }

  }
  component "Input3" is "Text Box"
  {
    Frame: (145, 60) - (190, 80)
    Style: tbfsNormal
    Disallow: "abcdefghijklmnopqrstuvwxz"

    connect 
    {
      signalComponentCreated -> parteditSink::RegisterInput3
      signalTextBoxLostFocus-> parteditSink::SetInput3
    }

  }
  component "Input4" is "Text Box"
  {
    Frame: (145, 85) - (190, 105)
    Style: tbfsNormal
    Disallow: "abcdefghijklmnopqrstuvwxz"

    connect 
    {
      signalComponentCreated -> parteditSink::RegisterInput4
      signalTextBoxLostFocus-> parteditSink::SetInput4
    }

  }
  component "Speed" is "Radio Button"
  {
    Frame: (195, 5) - (275, 20)
    Caption: "Speed"

    connect
    {
	signalRadButtonTurnedOn-> parteditSink::SetSpeed
    }
  }
  component "Acceleration" is "Radio Button"
  {
    Frame: (195, 20) - (300, 35)
    Caption: "Acceleration"

    connect
    {
	signalRadButtonTurnedOn -> parteditSink::SetAccel
    }
  }
  component "Position" is "Radio Button"
  {
    Frame: (195, 35) - (275, 50)
    Caption: "Position"

    connect
    {
	signalRadButtonTurnedOn -> parteditSink::SetPosition
    }
  }

  component "Rect" is "Radio Button"
  {
    Frame: (195, 50) - (275, 65)
    Caption: "Rectangle"

    connect
    {
	signalRadButtonTurnedOn -> parteditSink::SetRectangle
    }
  }
  component "NumParticle" is "Radio Button"
  {
    Frame: (195, 65) - (275, 80)
    Caption: "# Particles"

    connect
    {
	signalRadButtonTurnedOn -> parteditSink::SetNumParticle
    }
  }
  component "ParticleTime" is "Radio Button"
  {
    Frame: (195, 80) - (300, 95)
    Caption: "Particle Time"

    connect
    {
	signalRadButtonTurnedOn -> parteditSink::SetParticleTime
    }
  }
  component "AddAge" is "Radio Button"
  {
    Frame: (195, 95) - (275, 110)
    Caption: "Add Age"

    connect
    {
	signalRadButtonTurnedOn -> parteditSink::SetAddAge
    }
  }
}

