window "SectionSelection"
{
  Frame: (0,0) - (150,125)
  Style: wfsNormal
  Options: wfoGrip+wfoNoDrag

  connect
  {
    signalComponentCreated -> parteditSink::RegisterSectionSelection
  }
	
  component "SectionList" is "List Box"
  {
	  
    Frame: (0,0) - (138,108)
    Highlight: "/aws/lbhi.png"
    Columns: 1
    DefaultSortCOl: 0
    Column0Width: 190
    Column0Caption: "Sections"

    connect
    {
      signalComponentCreated -> parteditSink::FillSectionList
      signalListBoxStateChanged -> parteditSink::SectionListSelectionChanged
    }
  }	
}

