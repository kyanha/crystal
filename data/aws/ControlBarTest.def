window "Another"
{
  Frame: (100,100)-(600,440)
  Style: wfsNormal
  Title: "A window with lots of stuff"

component "Color 1" is "Image View"
{
  Frame: (20, 200)-(50,250)
  Color: 120, 178, 35
}

component "Color 2" is "Image View"
{
  Frame: (70, 200)-(100,250)
  Color: 120, 178, 35
}

component "Color 3" is "Image View"
{
  Frame: (120, 200)-(150,250)
  Color: 120, 178, 35
}

component "Color 4" is "Image View"
{
  Frame: (170, 200)-(200,250)
  Color: 120, 178, 35
}

component "Color 5" is "Image View"
{
  Frame: (220, 200)-(250,250)
  Color: 120, 178, 35
}

component "Color 6" is "Image View"
{
  Frame: (270, 200)-(300,250)
  Color: 120, 178, 35
}



component "Bar" is "Control Bar"
{

  Frame: (10,10)-(350,100)
  HorizontalGap: 2
  VerticalGap: 5
  Alignment: gcbAlignCenter
  Vertical: True


  component "Red" is "Command Button"
  {
    Frame: (20, 30) - (90, 50)
    Style: bfsNormal
    Caption: "Red"
    Toggle: Yes    

    connect 
    {
      signalCmdButtonClicked -> testButtonSink::RedClicked
    }
  }

  component "HideWindow" is "Command Button"
  {
    Frame: (100, 30) - (170, 50)
    Style: bfsNormal
    Caption: "Hide window"

    connect 
    {
      signalCmdButtonClicked -> awsStandardSink::HideWindow
    }
  }

   component "Male" is "Radio Button"
  {
    Frame: (170, 30) - (300, 50)
    Caption: "Male"
  }

  component "Sample" is "Bar Chart"
  {
   Frame: (320, 110) - (450, 200)
   Style: lbfsSunken
   Options: bcoVerticalChart


   connect 
    {
     signalComponentCreated -> testButtonSink::FillBarChart
    }
  }


  component "Users" is "List Box"
  {
    Frame: (350, 210) - (385, 350)
    Style: lbfsSunken
    Highlight: "/aws/lbhi.png"
    Columns: 3
    DefaultSortCOl: 0
    Column0Width: 90
    Column0Caption: "Class"
    Column1Width: 70
    Column1Caption: "Status"
    Column2Width: 100
    Column2Caption: "Name"
    Column2Align: lbAlignCenter

    connect 
    {
     signalComponentCreated -> testButtonSink::FillListBox
    }
  }
}




connect
{
  signalComponentCreated -> testButtonSink::ShowPallete
}
 
