    <sequences>
        <sequence name="fogloop">
	    <fadefog sector="kamer2l" red="1" green="1" blue="1" density="0.05"
	    	duration="1000" />
	    <delay>1000</delay>
	    <fadefog sector="kamer2l" red="1" green="0" blue="0" density="0.02"
	    	duration="1000" />
	    <delay>1000</delay>
	    <fadefog sector="kamer2l" red="0" green="1" blue="0" density="0.01"
		duration="1000" />
	    <delay>1000</delay>
	    <fadefog sector="kamer2l" red="0" green="0" blue="1" density="0.02"
	    	duration="1000" />
	    <delay>1000</delay>
	    <run sequence="fogloop" />
	</sequence>

        <sequence name="fog">
