	        <roty>1.5</roty>
                <v x="0" y="0.5" z="0" />
	    </rotate>
	    <rotate mesh="cubard:robotHead:ball2" duration="1000">
	        <rotz>2.2</rotz>
                <v x="0" y="0.5" z="0" />
	    </rotate>
	    <rotate mesh="cubard:robotHead:ball3" duration="1000">
	        <rotx>1</rotx>
                <v x="0" y="0.5" z="0" />
	    </rotate>

