                <v x="-16" y="-1.85" z="25" />
	    </rotate>
	    <delay>400</delay>
	    <rotate mesh="cube2" duration="300">
	        <roty>.5</roty>
                <v x="-16" y="-1" z="36" />
	    </rotate>
	    <delay>300</delay>
