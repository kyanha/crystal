	    </rotate>
	    <delay>300</delay>
	    <enable trigger="rot_shapes" />
	</sequence>

	<sequence name="rot_cubard">
	    <rotate mesh="cubard" duration="1000">
	        <roty>2</roty>
                <v x="4" y="0.3" z="12" />
	    </rotate>
