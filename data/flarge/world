	    <rotate mesh="cubard:robotArmLeft" duration="500">
	        <rotz>-.4</rotz>
                <v x="0" y="0.23" z="0.15" />
	    </rotate>
	    <delay>500</delay>
	    <rotate mesh="cubard:robotArmRight" duration="500">
	        <rotz>1.8</rotz>
                <v x="0" y="0.23" z="-0.15" />
	    </rotate>
	    <rotate mesh="cubard:robotArmLeft" duration="500">
	        <rotz>.4</rotz>
                <v x="0" y="0.23" z="0.15" />
	    </rotate>
	    <delay>500</delay>
