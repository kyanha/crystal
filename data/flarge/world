                            <m32>-0.00497519</m32>
                            <m33>0</m33>
                        </matrix>
                        <v x="0" y="3" z="38" />
                    </texmap>
                </p>
                <p name="down2E">
                    <v>13</v>
                    <v>16</v>
                    <v>15</v>
                    <v>12</v>
                    <texmap>
                        <matrix>
                            <m11>0.00497519</m11>
                            <m12>0.000497518</m12>
                            <m13>0</m13>
                            <m21>0</m21>
                            <m22>0</m22>
                            <m23>-0.005</m23>
                            <m31>0.000497519</m31>
                            <m32>-0.00497519</m32>
                            <m33>0</m33>
                        </matrix>
                        <v x="0" y="3" z="38" />
                    </texmap>
                </p>
            </params>
        </meshobj>
        <light>
            <center x="20" y="900" z="35" />
            <radius>10000</radius>
            <color red="1.3" green="1" blue="1.3" />
        </light>
    </sector>
    <sector name="wavySector">
