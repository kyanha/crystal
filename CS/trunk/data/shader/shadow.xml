
<shader type="xmlshader" name="shadowextrude">
	<technique priority="100">
		<pass>
			<mixmode><copy/></mixmode>
			<buffer name="shadow vertices" destination="attribute 0" />
			<buffer name="shadow normals" destination="attribute 1" />
			<writemask r="false" g="false" b="false" a="false"/>
			<vp plugin="glarb" file="/shader/shadowextrude.avp"/>
		</pass>
	</technique>
</shader>

