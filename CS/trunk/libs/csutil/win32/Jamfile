SubDir TOP libs csutil win32 ;

{
  local rejects ;
  if $(LIBBFD.AVAILABLE) != "yes"
  {
    rejects += callstack-bfd.cpp callstack-bfd.h ;
  }
  else
  {
    CSLIB.EXTERNAL_LIBS += LIBBFD ;
  }

  CsLibAddModule $(rejects) ;
}

CSUTIL.GENERIC +=
  appdir.cpp
  csprocessorcap.cpp
  findlib.cpp
  getopt.cpp
  pluginpaths.cpp
  resdir.cpp
  runloop.cpp ;
