SubDir TOP libs csws ;

Library csws :
	[ Wildcard *.cpp *.h ]
	[ Wildcard skins/default : *.cpp *.h ]
        [ Wildcard ../../include/csws : *.h ]
;

LibDepends csws : cstool csgfx csgeom csutil ;
Help csws : "Crystal Space Windowing System library" ;

InstallConfig csws.cfg ;
InstallData csws.zip ;

